        <asp:SqlDataSource ID="dsGridReportReksa" runat="server" ConnectionString="<%$ ConnectionStrings:ReksaAppConnectionStringDevX %>" SelectCommand="select  distinct a.Pid
	  , g.InvestmentType
	  , a.Transaction_Number 
	  , a.BuyDate
	  , c.InvestmentName
	  , d.CurrencyInitial, Company_Initial
	  , e.KursValue
	  , Price
	  , isnull(a.TotalUnit,0) as Beli
	  , (Price * a.TotalUnit) as AmountBuy
	  , (KursValue * Price * a.TotalUnit) as TotalAmountBuy
	  , b.SellDate
	  , isnull(b.TotalUnit,0) as jual
	  , isnull((select isnull(Price,0) from Reksa_MasterPriceInvestment x where x.pid=b.PriceInvestment_Pid),0) as HargaJual
	  , (Select KursValue from Reksa_MasterKurs y where y.pid=b.Kurs_Pid) as KursJual
	  , (select Price from Reksa_MasterPriceInvestment x where x.pid=b.PriceInvestment_Pid) * b.TotalUnit as AmountSell
	  , (select Price from Reksa_MasterPriceInvestment x where x.pid=b.PriceInvestment_Pid) * (Select KursValue from Reksa_MasterKurs y where y.pid=b.Kurs_Pid) * b.TotalUnit as TotalAmountJual
	  , b.Transaction_Number as nomorjual
	  , a.TotalUnit - isnull((select distinct sum(TotalUnit) TotalJual from Reksa_TRSell zz where zz.TRBuy_Pid=b.TRBuy_Pid),0) as Saldo
	  , case
			when ((select Price from Reksa_MasterPriceInvestment xx where xx.pid=b.PriceInvestment_Pid) - Price ) &lt; 0 then ((select Price from Reksa_MasterPriceInvestment xxx where xxx.pid=b.PriceInvestment_Pid) - Price) * (a.TotalUnit - (select distinct sum(TotalUnit) TotalJual from Reksa_TRSell zz where zz.TRBuy_Pid=b.TRBuy_Pid))
			when ((select Price from Reksa_MasterPriceInvestment xx where xx.pid=b.PriceInvestment_Pid) - Price ) &gt; 0 then ((select Price from Reksa_MasterPriceInvestment xxx where xxx.pid=b.PriceInvestment_Pid) - Price) * (a.TotalUnit - (select distinct sum(TotalUnit) TotalJual from Reksa_TRSell zz where zz.TRBuy_Pid=b.TRBuy_Pid))
			when ((select Price from Reksa_MasterPriceInvestment xx where xx.pid=b.PriceInvestment_Pid) - Price ) = 0 then ((select Price from Reksa_MasterPriceInvestment xxx where xxx.pid=b.PriceInvestment_Pid) - Price) * (a.TotalUnit - (select distinct sum(TotalUnit) TotalJual from Reksa_TRSell zz where zz.TRBuy_Pid=b.TRBuy_Pid))
			when isnull((select Price from Reksa_MasterPriceInvestment xxx where xxx.pid=b.PriceInvestment_Pid),0) = 0 then '0'
		end LossProfitValueByPrice
	  , case
			when ((Select KursValue from Reksa_MasterKurs y where y.pid=b.Kurs_Pid) - KursValue  ) &lt; 0 then ((Select KursValue from Reksa_MasterKurs y where y.pid=b.Kurs_Pid) - KursValue ) * (a.TotalUnit - (select distinct sum(TotalUnit) TotalJual from Reksa_TRSell zz where zz.TRBuy_Pid=b.TRBuy_Pid))
			when ((Select KursValue from Reksa_MasterKurs y where y.pid=b.Kurs_Pid) - KursValue  ) &gt; 0 then ((Select KursValue from Reksa_MasterKurs y where y.pid=b.Kurs_Pid) - KursValue ) * (a.TotalUnit - (select distinct sum(TotalUnit) TotalJual from Reksa_TRSell zz where zz.TRBuy_Pid=b.TRBuy_Pid))
			when ((Select KursValue from Reksa_MasterKurs y where y.pid=b.Kurs_Pid) - KursValue  ) = 0 then ((Select KursValue from Reksa_MasterKurs y where y.pid=b.Kurs_Pid) - KursValue ) * (a.TotalUnit - (select distinct sum(TotalUnit) TotalJual from Reksa_TRSell zz where zz.TRBuy_Pid=b.TRBuy_Pid))
			when isnull((Select KursValue from Reksa_MasterKurs y where y.pid=b.Kurs_Pid),0) = 0 then '0'
		end LossProfitValueByKurs
	  , (select top 1 Price from Reksa_MasterPriceInvestment xy where xy.InvestmentName_Pid=a.InvestmentName_Pid and isActive=1 order by EffectiveDate desc) as LastPrice
	  , (select top 1 KursValue from Reksa_MasterKurs xy where xy.Currency_Pid=a.Currency_Pid and isActive=1 order by Cdatetime desc) as LastKurs
	  , (a.TotalUnit - isnull((select distinct sum(TotalUnit) TotalJual from Reksa_TRSell zz where zz.TRBuy_Pid=a.Pid),0)) * ((select top 1 Price from Reksa_MasterPriceInvestment xy where xy.InvestmentName_Pid=a.InvestmentName_Pid and isActive=1 order by EffectiveDate desc) - Price) as UnrealizeGain,
	   ((select top 1 KursValue from Reksa_MasterKurs xy where xy.Currency_Pid=a.Currency_Pid and isActive=1 order by Cdatetime desc) - KursValue) * (a.TotalUnit - isnull((select distinct sum(TotalUnit) TotalJual from Reksa_TRSell zz where zz.TRBuy_Pid=a.Pid),0)) as Forex,
	   (select top 1 Price from Reksa_MasterPriceInvestment xy where xy.InvestmentName_Pid=a.InvestmentName_Pid and isActive=1 order by EffectiveDate desc) * (a.TotalUnit - isnull((select distinct sum(TotalUnit) TotalJual from Reksa_TRSell zz where zz.TRBuy_Pid=a.Pid),0)) as LastAmount,
	   (select top 1 KursValue from Reksa_MasterKurs xy where xy.Currency_Pid=a.Currency_Pid and isActive=1 order by Cdatetime desc) * (select top 1 Price from Reksa_MasterPriceInvestment xy where xy.InvestmentName_Pid=a.InvestmentName_Pid and isActive=1 order by EffectiveDate desc) * (a.TotalUnit - isnull((select distinct sum(TotalUnit) TotalJual from Reksa_TRSell zz where zz.TRBuy_Pid=a.Pid),0)) as LastTotalAmount
from Reksa_TRBuy a
left join Reksa_TRSell b on b.TRBuy_Pid = a.Pid
inner join Reksa_MasterInvestmentName c on a.InvestmentName_Pid=c.Pid
inner join Reksa_MasterCurrency d on a.Currency_Pid=d.Pid 
inner join Reksa_MasterKurs e on a.Kurs_Pid=e.Pid 
inner join Reksa_MasterPriceInvestment f on a.PriceInvestment_Pid=f.Pid
inner join Reksa_MasterInvestmentType g on c.InvestmentType_Pid=g.Pid
inner join Reksa_MasterCompany h on c.Company_Pid=h.pid">
            <%--<SelectParameters>
                <asp:ControlParameter ControlID="cboCompany" DefaultValue="0" Name="Company_Pid" PropertyName="Value" />
            </SelectParameters>--%>
        </asp:SqlDataSource>
        <asp:SqlDataSource ID="dsCBOCompany" runat="server" ConnectionString="<%$ ConnectionStrings:ReksaAppConnectionStringDevX %>" SelectCommand="SELECT [Pid], [Company_Name] FROM [Reksa_MasterCompany] WHERE ([isActive] = @isActive)">
            <SelectParameters>
                <asp:Parameter DefaultValue="true" Name="isActive" Type="Boolean" />
            </SelectParameters>
        </asp:SqlDataSource>
    </section>
